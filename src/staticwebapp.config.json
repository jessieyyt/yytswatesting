{
  "login": {
    "tokenStore": {
      "enabled": true,
      "tokenRefreshExtensionHours": 72,
      "azureBlobStorage": {
        "sasUrlSettingName": "WEBSITE_AUTH_TOKEN_CONTAINER_SASURL"
      }
    }
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  }
}

